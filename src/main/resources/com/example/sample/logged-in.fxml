<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sample.LoggedInController">
    <children>
        <Label fx:id="lable_welcome" layoutX="170.0" layoutY="54.0" prefHeight="70.0" prefWidth="276.0" text="Choose game!" textAlignment="RIGHT" textFill="#6a99f7" wrapText="true">
            <font>
                <Font name="Comic Sans MS Bold" size="36.0" />
            </font>
            <effect>
                <Glow />
            </effect>
        </Label>
        <Button fx:id="button_logout" layoutX="207.0" layoutY="311.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="188.0" style="-fx-background-color: #6a99f7;" text="Logout" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS Bold" size="17.0" />
            </font>
        </Button>
        <Button fx:id="button_wordle" layoutX="108.0" layoutY="217.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="144.0" style="-fx-background-color: #8328CC;" text="Wordle" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="24.0" />
            </font>
        </Button>
        <Button fx:id="button_flags" layoutX="347.0" layoutY="217.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="144.0" style="-fx-background-color: #8328CC;" text="Flags" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS Bold Italic" size="24.0" />
            </font>
        </Button>
        <ImageView fx:id="im_wordle" fitHeight="88.0" fitWidth="98.0" layoutX="130.0" layoutY="128.0" pickOnBounds="true" preserveRatio="true" />
        <ImageView fx:id="im_flags" fitHeight="88.0" fitWidth="112.0" layoutX="366.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
